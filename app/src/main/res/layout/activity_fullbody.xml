<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fullbody"
    android:background="#F5F5F5"
    >
    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:rowCount="20"
        android:columnCount="1"
        >

        <TextView
            android:id="@+id/title4"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:fontFamily="@font/lexendsemibold"
            android:gravity="center"
            android:text="Full Body"
            android:textSize="40dp"
            android:textColor="@color/black"
            />

        <TextView
            android:id="@+id/t30"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textColor="@color/black"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/t31"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/days"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/lexendregular"
            android:text="3 Days week"
            android:textColor="@color/black"
            android:textSize="22dp" />
        <TextView
            android:id="@+id/t32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:visibility="invisible" />
        <TextView
            android:id="@+id/d1"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="#1ED9D9D9"
            android:fontFamily="@font/lexendregular"
            android:gravity="center"
            android:text="Farmer's Carry Dumbbell : 4x12"
            android:textColor="@color/black"
            android:textSize="25dp" />
        <TextView
            android:id="@+id/t33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:visibility="invisible" />
        <TextView
            android:id="@+id/deadlift"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="#1ED9D9D9"
            android:fontFamily="@font/lexendregular"
            android:gravity="center"
            android:text="Deadlift Barbell : 4x8"
            android:textColor="@color/black"
            android:textSize="25dp" />
        <TextView
            android:id="@+id/t34"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:visibility="invisible" />
        <TextView
            android:id="@+id/Bench"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="#1ED9D9D9"
            android:fontFamily="@font/lexendregular"
            android:gravity="center"
            android:text="Bench Press Barbell : 4x8"
            android:textColor="@color/black"
            android:textSize="25dp" />
        <TextView
            android:id="@+id/t35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:visibility="invisible" />
        <TextView
            android:id="@+id/bent"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="#1ED9D9D9"
            android:fontFamily="@font/lexendregular"
            android:gravity="center"
            android:text="Bent-over Row Barbell : 4x8"
            android:textColor="@color/black"
            android:textSize="25dp" />
        <TextView
            android:id="@+id/t36"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:visibility="invisible" />
        <TextView
            android:id="@+id/overhead"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="#1ED9D9D9"
            android:fontFamily="@font/lexendregular"
            android:gravity="center"
            android:text="Overhead Press Barbell : 4x8"
            android:textColor="@color/black"
            android:textSize="25dp" />
        <TextView
            android:id="@+id/t37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:visibility="invisible" />
        <TextView
            android:id="@+id/ab"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="#1ED9D9D9"
            android:fontFamily="@font/lexendregular"
            android:gravity="center"
            android:text="Ab Wheel : 4x8"
            android:textColor="@color/white"
            android:textSize="25dp" />
    </GridLayout>



</androidx.constraintlayout.widget.ConstraintLayout>